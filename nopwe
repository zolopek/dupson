def dodowanie(x, y):
    print(x+y)

dodowanie(2, 3)

def dodowanie2(x, y, z = 0):
    print(x+y+z)

dodawanie2(2, 3 , 5)
dodawanie2(5, 6)
dodowanie2(9, 5)

